{
  "name": "cross-browser-tests-runner",
  "version": "0.5.3",
  "description": "Seamless cross-browser testing with multiple platforms and runners",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cross-browser-tests-runner/cross-browser-tests-runner.git"
  },
  "keywords": [
    "cross-browser testing",
    "javascript testing",
    "javascript unit testing",
    "JS testing",
    "browserstack",
    "saucelabs",
    "testem",
    "jasmine"
  ],
  "author": "Reetesh Ranjan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cross-browser-tests-runner/cross-browser-tests-runner/issues"
  },
  "homepage": "https://github.com/cross-browser-tests-runner/cross-browser-tests-runner#readme",
  "files": [
    "lib",
    "bin",
    "conf",
    "samples"
  ],
  "bin": {
    "cbtr-server": "./bin/server/server.js",
    "cbtr-quick-start": "./bin/utils/quick-start.js",
    "cbtr-browserstack-update": "./bin/utils/conf/browsers/browserstack.js",
    "cbtr-saucelabs-update": "./bin/utils/conf/browsers/saucelabs.js",
    "cbtr-init": "./bin/utils/settings/cbtr.js",
    "cbtr-testem-browserstack-init": "./bin/utils/settings/testem/browserstack.js",
    "cbtr-testem-browserstack-open": "./bin/hooks/testem/browserstack/open.js",
    "cbtr-testem-browserstack-close": "./bin/hooks/testem/browserstack/close.js",
    "cbtr-testem-browserstack-browser": "./bin/hooks/testem/browserstack/browser.js",
    "cbtr-testem-saucelabs-init": "./bin/utils/settings/testem/saucelabs.js",
    "cbtr-testem-saucelabs-open": "./bin/hooks/testem/saucelabs/open.js",
    "cbtr-testem-saucelabs-close": "./bin/hooks/testem/saucelabs/close.js",
    "cbtr-testem-saucelabs-browser": "./bin/hooks/testem/saucelabs/browser.js"
  },
  "scripts": {
    "eslint": "./node_modules/.bin/eslint bin/ lib/",
    "unx-test-command": "LOG_LEVEL=WARN ./node_modules/.bin/istanbul cover --handle-sigint --include-pid ./node_modules/mocha/bin/_mocha -- --reporter lcovonly --ui bdd -R spec -t 5000 $TEST_PATH",
    "unit-tests": "TEST_PATH=tests/unit/tests.js npm run unx-test-command",
    "unit-tests-1": "TEST_PATH=tests/unit/tests-1.js npm run unx-test-command",
    "unit-tests-2": "TEST_PATH=tests/unit/tests-2.js npm run unx-test-command",
    "unit-tests-3a": "TEST_PATH=tests/unit/tests-3a.js npm run unx-test-command",
    "unit-tests-3b": "TEST_PATH=tests/unit/tests-3b.js npm run unx-test-command",
    "integration-tests": "TEST_PATH=tests/integration/tests.js npm run unx-test-command",
    "functional-tests": "TEST_PATH=tests/functional/tests.js npm run unx-test-command",
    "eslint-unit-tests-1": "npm run eslint && npm run unit-tests-1",
    "win-test-command": "./node_modules/.bin/istanbul cover --handle-sigint --include-pid ./node_modules/mocha/bin/_mocha -- --reporter lcovonly --ui bdd -R spec -t 5000 %TEST_PATH%",
    "unit-tests-win": "cmd /v /c \"set TEST_PATH=tests/unit/tests.js&& npm run win-test-command\"",
    "unit-tests-win-1": "cmd /v /c \"set TEST_PATH=tests/unit/tests-1.js&& npm run win-test-command\"",
    "unit-tests-win-2": "cmd /v /c \"set TEST_PATH=tests/unit/tests-2.js&& npm run win-test-command\"",
    "unit-tests-win-3a": "cmd /v /c \"set TEST_PATH=tests/unit/tests-3a.js&& npm run win-test-command\"",
    "unit-tests-win-3b": "cmd /v /c \"set TEST_PATH=tests/unit/tests-3b.js&& npm run win-test-command\"",
    "integration-tests-win": "cmd /v /c \"set TEST_PATH=tests/integration/tests.js&& npm run win-test-command\"",
    "functional-tests-win": "cmd /v /c \"set TEST_PATH=tests/functional/tests.js&& npm run win-test-command\"",
    "eslint-unit-tests-win-1": "npm run eslint && npm run unit-tests-win-1",
    "codecov-upload": "./node_modules/.bin/codecov",
    "coveralls-upload": "cat coverage/lcov.info | ./node_modules/.bin/coveralls",
    "coveralls-upload-win": "node node_modules\\coveralls\\bin\\coveralls-win.js coverage\\lcov.info"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=2.15.11"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^3.0.0",
    "chai-spies": "^0.7.1",
    "codecov": "^2.3.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.8.0",
    "finalhandler": "^1.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "nock": "^9.0.22",
    "serve-static": "^1.13.1",
    "testem": "^1.18.4"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "decompress": "^4.2.0",
    "decompress-targz": "^4.1.1",
    "decompress-unzip": "^4.0.1",
    "express": "^4.16.2",
    "js-yaml": "^3.9.1",
    "minimist": "^1.2.0",
    "p-retry": "^1.0.0",
    "ps-node": "^0.1.6",
    "request-promise": "^4.2.2",
    "selenium-webdriver": "^3.6.0",
    "useragent": "^2.2.1",
    "uuid": "^3.1.0"
  }
}
